package com.scb.assignment.bookStoreRestApi.jsonObject;

public class OrderResult {
	private String price;

	public OrderResult(String price) {
		super();
		this.price = price;
	}
	
	public OrderResult(Integer price) {
		super();
		this.price = (price/100) + "." + (price%100) ;
	}

	public String getPrice() {
		return price;
	}

	public void setPrice(String price) {
		this.price = price;
	}

	@Override
	public String toString() {
		return "OrderResult [price=" + price + "]";
	}
	
	
}
