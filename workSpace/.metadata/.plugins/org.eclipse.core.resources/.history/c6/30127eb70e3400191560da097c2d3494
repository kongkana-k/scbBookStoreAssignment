package com.scb.assignment.bookStoreRestApi.utility;

import java.nio.charset.StandardCharsets;
import java.security.MessageDigest;
import java.security.NoSuchAlgorithmException;

public class CryptographUtility {
	
	public static String shaToBase64(String originalString) throws NoSuchAlgorithmException {
		MessageDigest digest = MessageDigest.getInstance("SHA-256");
		digest.digest(originalString.getBytes(StandardCharsets.UTF_8));
		return "";
	}
	
}
